@page
@model MathGame.Pages.Games.ScoresModel
@{
}

<body class="imgBackground text-center" style="background-image: url('../images/scores.png');">
    <form class="startForm">
        <table>
            <tr>
                <th>Name</th>
                <th>Age</th>
                <th>Score</th>
            </tr>
            @foreach (var p in Model.PlayerList)
            {
                <tr>
                    <td>@p.Name</td>
                    <td>@p.Age</td>
                    <td>@p.Score</td>
                </tr>
            }
        </table>
    </form>
</body>